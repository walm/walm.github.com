---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Andreas Wålm</title>
 <link href="http://andreas.walm.se/atom.xml" rel="self"/>
 <link href="http://andreas.walm.se/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://andreas.walm.se/</id>
 <author>
   <name>Andreas Wålm</name>
   <email>andreas@walm.net</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://andreas.walm.se{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://andreas.walm.se{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
